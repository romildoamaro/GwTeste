select array_to_string(array_agg(descricao order by descricao), ', ')
from type_services group by id
where id = 53066 group by id

select descricao from type_services  
LEFT JOIN (SELECT ts.id
, array_agg(ts.descricao::varchar ORDER BY ts.descricao, ',') as descricao_servico
FROM type_services ts GROUP BY ts.id
		   
